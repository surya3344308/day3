#include<stdio.h>
int main(){
    int m1,m2,m3,m4,m5;
    printf("enter 5 marks");
    scanf("%d %d %d %d %d",&m1,&m2,&m3,&m4,&m5);
    int sum=m1+m2+m3+m4+m5;
    int avg=sum/5;
    printf("sum=%d,average=%d",sum,avg);
    if(avg>90 && avg<=100){
        printf("a grade");
    } else if(avg>80 && avg<=90){
        printf("b grade");
    } else if(avg>70 && avg<=80){
        printf("c grade");
    } else if(avg>60 && avg<=70){
        printf("d grade");
    } return 0;
    }
